<script>
    let f = document.getElementById("Featured");
    let o = document.getElementById("OnSales");
    let t = document.getElementById("TopReted");
    let fItem = document.getElementById("FeaturedItes");
    let oItem = document.getElementById("OnsaleItems");
    let tItem = document.getElementById("RatingItems");
    f.addEventListener("click", () => {
        f.classList.add("active");
        o.classList.remove("active");
        t.classList.remove("active");
        fItem.style.display = "block";
        oItem.style.display = "none";
        tItem.style.display = "none";
    })
    o.addEventListener("click", () => {
        f.classList.remove("active");
        o.classList.add("active");
        t.classList.remove("active");
        fItem.style.display = "none";
        oItem.style.display = "block";
        tItem.style.display = "none";
    })
    t.addEventListener("click", () => {
        f.classList.remove("active");
        o.classList.remove("active");
        t.classList.add("active");
        fItem.style.display = "none";
        oItem.style.display = "none";
        tItem.style.display = "block";
    })
    let deal = document.getElementsByClassName("realted_deal_img");

    for (let i = 0; i < deal.length; i++) {
        deal[i].addEventListener("click", (e) => {
            document.getElementById("dealImg").src = e.target.src;
            for (let i = 0; i < deal.length; i++) {
                deal[i].classList.remove("active")

            }
            e.target.parentElement.classList.add("active")
        })
    }

    //Bestsaller
    let top20 = document.getElementById("top20");
    let Phones_Tablet = document.getElementById("Phones_Tablet");
    let Laptop_Computer = document.getElementById("Laptop_Computer");
    let cameras = document.getElementById("cameras");
    let cBtn = document.getElementsByClassName("roundBtn");
    let activeBox = document.getElementsByClassName("boxActive");
    cBtnVal = cBtn.length;
    for (let i = 0; i < cBtnVal; i++) {
        cBtn[i].addEventListener("click", (e) => {
            let target = e.target;
            for (let i = 0; i < cBtnVal; i++) {
                cBtn[i].classList.remove("active");

            }
            target.classList.add("active");
            for (let j = 0; j < activeBox.length; j++) {
                for (let j = 0; j < activeBox.length; j++) {
                    if (target.innerHTML == activeBox[j].innerHTML) {
                        activeBox[j].parentElement.classList.remove("hide");
                    }
                    else {
                        activeBox[j].parentElement.classList.add("hide");

                    }
                }
            }
        })
    }

</script>
